#include <stdio.h>

// Function to calculate sum of array elements
int sumArray(int arr[], int n) {
    int sum = 0;

    for(int i = 0; i < n; i++) {
        sum = sum + arr[i];
    }

    return sum;
}

int main() {
    int arr[5] = {10, 20, 30, 40, 50};
    int result;

    // Function call
    result = sumArray(arr, 5);

    printf("Sum of array elements = %d", result);
    return 0;
}