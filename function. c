#include <stdio.h>

// Function declaration
int add(int a, int b);

int main() {
    int x, y, result;

    printf("Enter two numbers: ");
    scanf("%d %d", &x, &y);

    // Function call
    result = add(x, y);

    printf("Sum = %d", result);

    return 0;
}

// Function definition
int add(int a, int b) {
    return a + b;
}