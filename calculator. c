#include <stdio.h>
int main() {
    char op;
    int a, b;
    scanf("%c", &op);
    scanf("%d %d", &a, &b);

    switch(op) {
        case '+': printf("Result = %d", a + b); break;
        case '-': printf("Result = %d", a - b); break;
        case '*': printf("Result = %d", a * b); break;
        case '/': printf("Result = %d", a / b); break;
        default: printf("Invalid operator");
    }
    return 0;
}